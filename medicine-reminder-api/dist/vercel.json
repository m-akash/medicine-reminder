{
    "version": 2,
    "builds": [
        {
            "src": "index.ts",
            "use": "@vercel/node"
        },
        {
            "src": "api/**/*.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1.ts"
        },
        {
            "src": "/(.*)",
            "dest": "/index.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ],
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "https://mediping.netlify.app"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET,POST,PUT,DELETE,OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "Content-Type, Authorization"
                }
            ]
        }
    ]
}
