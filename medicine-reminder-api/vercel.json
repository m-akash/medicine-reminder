{
    "version": 2,
    "builds": [
        {
            "src": "api/sendMedicineReminders.ts",
            "use": "@vercel/node"
        },
        {
            "src": "index.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/sendMedicineReminders",
            "dest": "/api/sendMedicineReminders.ts",
            "methods": [
                "GET",
                "POST"
            ]
        },
        {
            "src": "/(.*)",
            "dest": "/index.ts",
            "methods": [
                "GET",
                "POST",
                "PUT",
                "PATCH",
                "DELETE",
                "OPTIONS"
            ]
        }
    ]
}